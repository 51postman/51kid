								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"code is 10000\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.code).to.eql(\"10000\")",
									"});",
									"",
									"// 验证返回时间点",
									"// 无时间点时，timetable_list返回空，不会有time_str字段",
									"pm.test(\"返回OC课约课时间点\", function () {",
									"    pm.expect(pm.response.text()).to.include(\"time_str\");",
									"});",
									"/*",
									"避免约课浪费账号",
									"1、从slot天数的最晚一天（一般是第7天）倒数遍历",
									"2、寻找最晚开课的时间点",
									"3、获取开课时间点的courseId，设置环境变量，供约课接口使用",
									"*/",
									"var jsonData = pm.response.json();",
									"// 定义缩写",
									"var slotList = jsonData.res.slot_list;  ",
									" // 获取slot的长度，一般都是7",
									"var slotLength = jsonData.res.slot_list.length;",
									"",
									"// 从最晚的一天到当天，寻找可以约课的时间点",
									"// 如果存在可约课时间点，使用length判断不为空",
									"// 获取最晚的开课时间点，并返回它的courseId",
									"for (let i = slotLength-1; i>=0; i-- ){",
									"    var timeTableLength = slotList[i].timetable_list.length;",
									"    if (timeTableLength > 0){",
									"        var courseId = slotList[i].timetable_list[timeTableLength - 1].course_id;",
									"        // console.log('courseId', courseId)",
									"        pm.environment.set(\"classId\", courseId);",
									"        break;",
									"    }",
									"}",
									"// 测试完成，清除变量",
									"pm.environment.unset(\"days\");"
								],
								"type": "text/javascript"
							}
