							"function getNum(num){",
							"    if (parseInt(num) < 10){",
							"        num = '0' + num;",
							"    }",
							"    return num;",
							"}",
							"// 获取当前日期+6，即第七天的日期",
							"var dayAdd =new Date().setDate(new Date().getDate() + 6);",
							"var newDate = new Date(dayAdd);",
							"var oYear = newDate.getFullYear(),",
							"    oMonth = newDate.getMonth()+1,",
							"    oDay = newDate.getDate();",
							"// 组装日期，如果月份、日期小于10，则补字符串‘0’",
							"var oTime = oYear +'-'+ getNum(oMonth) +'-'+ getNum(oDay);",
							"",
							"pm.environment.set(\"date\", oTime);",
							"",
							""
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "642324d1-ee47-4430-bb82-76846a200a59",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"pm.test(\"code is '1'\", function () {",
							"    pm.expect(jsonData.code).to.eql(\"1\");",
							"});",
							"",
							"if(jsonData.res.totalPageNum > 0){",
							"    tests[\"查询老师返回正确\"] = true;",
							"    // 设置name，供按老师名称搜索使用",
							"    /********************************************",
							"     * ******************************************",
							"     * 注意， 此处设置环境变量【name\\tid】，注意清除 ",
							"     * ******************************************",
							"     * *****************************************/",
							"    pm.environment.set(\"name\", jsonData.res.remindMsg[0].realName);",
							"    pm.environment.set(\"tid\", jsonData.res.remindMsg[0].teaId+\"\");",
							"}else{",
