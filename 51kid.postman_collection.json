						"id": "642324d1-ee47-4430-bb82-76846a200a59",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"var jsonData = pm.response.json();",
							"pm.test(\"code is 1\", function () {",
							"    pm.expect(jsonData.code).to.eql(\"1\");",
							"});",
							"// 定义格式化时间函数（传参需要标准数据，如：07:00）",
							"function formatTime(num){",
							"    if(parseInt(num) < 10){",
							"        num = '0' + num;",
							"    }",
							"    return num;",
							"}",
							"",
							"// 接口返回的defaultTime为秒，js时间戳为毫秒",
							"function defaultTime(second){",
							"    var defaultTimeStamp = (jsonData.res.defaultTime + second) * 1000; ",
							"    var newHour = new Date(defaultTimeStamp).getHours();",
							"    var newMin = new Date(defaultTimeStamp).getMinutes();",
							"    var defaultHourAndMin = formatTime(newHour) +\":\"+ formatTime(newMin);",
							"    return defaultHourAndMin;",
							"}",
							"",
							"if (jsonData.code === '1'){",
							"    // 验证存在返回的老师，并在环境变量设置格式化后的时间",
							"    if (jsonData.res.remindMsg[0].teaId !== \"\" ){",
							"        tests[\"exist teaid\"] = true;",
							"        // 设置默认搜索时间，供多条件搜索接口使用",
							"        pm.environment.set(\"time\", defaultTime(0));",
							"    }",
							"}",
							"else if(jsonData.code === '202'){",
							"        // 202一般是课约满了，则查询半小时后的数据",
							"        pm.environment.set(\"time\", defaultTime(1800));",
							"        tests[\"exist teaid\"] = true;",
							"    }",
							"else{",
							"        tests[\"exist teaid\"] = false;",
							"    }"
						],
						"type": "text/javascript"
					}
				}
			],
