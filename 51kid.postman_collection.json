							"tests[\"Status code is 200\"] = responseCode.code === 200;",
							"if(responseCode.code === 200){",
							"var jsonData =JSON.parse(responseBody);//获取body中返回的所有参数",
							"if (jsonData.code == 1) {",
							"  //如果有则此次验证通过",
							"  tests[\"code is 1 \"] = true;",
							"  //获取需要的参数",
							" pm.environment.set(\"userId\",jsonData.res.userId);//把返回参数中的keys设置为环境变量",
							" pm.environment.set(\"talk_token\",jsonData.res.talkToken);//把返回参数中的keys设置为环境变量",
							"}",
							"else {",
							"  //如果无则此次验证不通过",
							"  tests[\"code is 1\"] = false;",
							"}",
							"}"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "c40656b0-b189-4973-b029-8b6eb7267828",
						"exec": [
							"var nowTime = Math.round(Date.now()/1000);",
							"pm.environment.set(\"timestamp\",nowTime);",
							"",
							"//1、 定义指定源串1",
							"var sourceString = '!08za$8gQKZCt^ME';",
							"",
							"//2、 获取源串2，按所有请求参数的key升序排列，然后将所有值（key=value）通过&符号连起来",
							"// 2.1 获取post中的所有请求参数和值",
							"// 2.2 获取get中的所有请求参数和值",
							"var param = request.data;   //post 参数",
							"var reqGetJson = pm.request.url.query.members;  //get中的参数",
							"",
							"// 反序列化body里面的参数为json",
							"try {",
							"  var json = JSON.parse(param);",
							"  var reqPostJson = json;",
							"  }catch(err){",
							"  //BODY不是JSON格式",
							"  }",
							"console.log()",
							"var keyArr = [];",
							"// 获取get参数的key",
							"for(let key in reqGetJson){",
							"    keyArr.push(key);",
							"}",
							"// 获取post参数的key",
							"for(let key in reqPostJson){",
							"    keyArr.push(key);",
							"}",
							"// 对合并后的key进行升序排列",
							"var keySort = keyArr.sort();",
							"var kvArr = [];",
							"// 编码，并组装所有的参数（key=value）",
							"for (let i of keySort){",
							"    if (i === 'tsign'){",
							"        continue;",
							"    }",
							"    else if (reqGetJson.hasOwnProperty(i)){",
							"        kvArr.push(i+\"=\"+encodeURIComponent(reqGetJson[i]))",
							"    }else{",
							"        kvArr.push(i+\"=\"+encodeURIComponent(reqPostJson[i]))",
							"    }",
							"}",
							"// 从数组转换为string",
							"var sign = kvArr.join(\"&\");",
							"sign = sign + sourceString;",
							"// 使用md5，生成32为加密的字符串",
							"var signCryp = CryptoJS.MD5(sign).toString();",
							"console.log('the sign is', signCryp);",
							"pm.environment.set(\"tsign\", signCryp);",
							"",
							"var password = \"111111\"",
							"pm.environment.set(\"password\",CryptoJS.MD5(password).toString());",
							""
						],
						"type": "text/javascript"
					}
				}
			],
